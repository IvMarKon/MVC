{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script",
        "ecmaFeatures": {}
    },
    "rules": {
        "indent_style":"space",
        "index_size": 2,
        "end_of_line": "lf",
        "charset": "utf-8",
        "trim_trailing_whitespace": "true",
        "insert_final_newline": "true"
    },
    "env": {
        "jquery": true,
        "browser": true,
        "node": true
    }
}